#include <iostream>
using namespace std;

sumofsquares(int n){
    int s=0;
    int i;
    for(i=1; i<=n; i++){
        s=s+i*i;
    }
    cout<<s;
}

int main(){
    int n;
    cout<<"Enter a number ";
    cin>>n;
    cout<<"The sum of squares till the entered number is ";
    sumofsquares(n);
    return 0;

}
